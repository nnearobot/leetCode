# 2877. Create a DataFrame from List
# easy
# https://leetcode.com/problems/create-a-dataframe-from-list/

import pandas as pd

def createDataframe(student_data: List[List[int]]) -> pd.DataFrame:
    column_names = [
        "student_id",
        "age"
    ]
    df = pd.DataFrame(student_data, columns=column_names)

    return df
